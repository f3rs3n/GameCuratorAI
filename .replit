modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["libGL", "python-launcher"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DAT Filter AI Headless"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DAT Filter AI Interactive"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "DAT Filter AI Gemini Test"

[[workflows.workflow]]
name = "DAT Filter AI Headless"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python headless.py --input \"test_input/Atari - Atari 5200 (20250316-080333) (Retool 2025-03-21 11-57-31) (77) (-ny) [-aABbcdkMmpPv].dat\" --output \"filtered_atari5200_random.dat\" --provider random --report \"report_atari5200_random.json\" --summary \"summary_atari5200_random.txt\" --batch-size 10"

[[workflows.workflow]]
name = "DAT Filter AI Interactive"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python interactive.py"

[[workflows.workflow]]
name = "DAT Filter AI Gemini Test"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python headless.py --input \"ToFilter/Atari - Atari 2600 (20250311-005439) (Retool 2025-03-21 11-57-27) (562) (-ny) [-aABbcdkMmpPv].dat\" --output \"test_gemini_atari2600.dat\" --provider gemini --report \"report_gemini_atari2600.json\" --summary \"summary_gemini_atari2600.txt\" --batch-size 20"

[deployment]
run = ["sh", "-c", "python main.py"]
